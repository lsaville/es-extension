{
  "manifest_version": 2,
  "name": "EventStore Highlighter",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [ "src/css/es-highlighter.css" ],
      "js": [ "src/js/content-script.js" ]
    }
  ],
  "web_accessible_resources": [ "src/js/*" ]
}
